{
  "name": "health-welness-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:sentry": "sentry-cli releases new $npm_package_version && tsc -b && vite build && sentry-cli releases files $npm_package_version upload-sourcemaps dist && sentry-cli releases finalize $npm_package_version",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "mock-server": "node mock-server/server.js",
    "dev:full": "concurrently \"npm run mock-server\" \"npm run dev\"",
    "sentry:test": "node -e \"import('./src/services/sentry-utils.js').then(m => m.runAllSentryTests())\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@sentry/react": "^10.8.0",
    "@stripe/react-stripe-js": "^3.9.2",
    "@stripe/stripe-js": "^7.9.0",
    "@types/node": "^24.3.0",
    "express": "^5.1.0",
    "framer-motion": "^12.23.12",
    "i18next": "^25.4.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-router-dom": "^7.8.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@sentry/cli": "^2.53.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "gtag": "^1.0.1",
    "jsdom": "^26.1.0",
    "json-server": "^1.0.0-beta.3",
    "prettier": "^3.6.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
